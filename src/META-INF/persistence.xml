<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="AWT_DB">
	<jta-data-source>AWT_DB</jta-data-source>
	<class>com.ibm.awt.data.Workflow</class>
	<class>com.ibm.awt.data.AwtHistory</class>
	<class>com.ibm.awt.data.AwtFile</class>
		<properties>
			<property name="eclipselink.jdbc.batch-writing" value="JDBC"/>
			<property name="eclipselink.jdbc.batch-writing.size" value="8192"/>
		</properties>
	</persistence-unit>
</persistence>
